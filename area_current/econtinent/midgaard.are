  mpmset self quest 12 4 0
  if quest (4,1,$n) == 1
    mpmadd $i quest 12 4 1
  endif
  if quest (5,1,$n) == 1
    mpmadd $i quest 12 4 1
  endif
  if quest (6,1,$n) == 1
    mpmadd $i quest 12 4 1
  endif
  if quest (7,1,$n) == 1
    mpmadd $i quest 12 4 1
  endif
  if quest (12,4,$i) == 0
    say You have no votes.
    break
  endif
  if quest (12,4,$i) == 1
    say You have one vote.
  endif
  if quest (12,4,$i) == 2
    say You have two votes.
  endif
  if quest (12,4,$i) == 3
    say You have three votes.
  endif
  if quest (12,4,$i) == 4
    say You have four votes.
  endif
  say If you wish to clear it, say CLEAR.
endif
